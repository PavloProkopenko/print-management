generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id       Int    @id(map: "User_pkey") @default(autoincrement())
  email    String @unique(map: "User_email_key")
  password String
  role     String @default("user")
}
